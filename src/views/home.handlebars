
<h1>Home</h1>

<div class="container">
    <div class="row">
        {{#each products}}
            <div class="col-4">
                <div class="card" style="width: 18rem;">
                    <div class="card-body">
                        <h5 class="card-title">{{this.title}}</h5>
                        <p class="card-text">{{this.description}}</p>
                        <p class="card-text"><small class="text-muted">Precio: {{this.price}}</small></p>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>









